<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>BukuServlet</servlet-name>
        <servlet-class>controller.BukuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KategoriServlet</servlet-name>
        <servlet-class>controller.KategoriServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransaksiServlet</servlet-name>
        <servlet-class>controller.TransaksiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProsesTransaksiServlet</servlet-name>
        <servlet-class>controller.ProsesTransaksiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BukuServlet</servlet-name>
        <url-pattern>/BukuServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KategoriServlet</servlet-name>
        <url-pattern>/KategoriServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransaksiServlet</servlet-name>
        <url-pattern>/TransaksiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProsesTransaksiServlet</servlet-name>
        <url-pattern>/ProsesTransaksiServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
